{
    "name": "dilbrinazad/laravel-multi-lang",
    "description": "A Laravel package for multilingual support",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dilbrinazad\\LaravelMultiLang\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dilbrinazad\\LaravelMultiLang\\Providers\\LaravelMultiLangServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "dilbrin"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
